<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.badmintonscoreboard" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SingleScore"
            android:label="Single Scoreboard"
            android:theme="@style/AppTheme" />
        <activity android:name=".SingleScore2"
            android:label="Single Scoreboard"
            android:theme="@style/AppTheme" />
        <activity android:name=".SingleScore3"
            android:label="Single Scoreboard"
            android:theme="@style/AppTheme" />
        <activity android:name=".DoubleReg"
            android:label="Double Register"
            android:theme="@style/AppTheme" />
        <activity android:name=".DoubleScore"
            android:label="Double Score"
            android:theme="@style/AppTheme" />
    </application>

</manifest>
